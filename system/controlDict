FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     MTO_TF;

startFrom       startTime;//latestTime;

startTime       0;

stopAt          endTime;

endTime         1.0;

deltaT          0.002;

writeControl    timeStep;

writeInterval   50;

purgeWrite      0;

writeFormat     ascii;

writePrecision  12;

writeCompression off;

timeFormat      general;

timePrecision   12;

// adjustTimeStep  yes;

// maxCo           10;

runTimeModifiable true;
libs ( "libOpenFOAM.so" "libgroovyBC.so" ) ;
