relaxationFactors
{
    fields
    {
        "(p|pa|pb|pc)"     0.3;
         alpha             0.1;
    }
    equations
    {
        "(U|Ua|Ub|Uc)"     0.7;
        "(k|epsilon)"      0.7;
    }

}


power_dissipation
{
    numberConstraintPatches      2;
    constraintPatchesNames       (inlet outlet);
}

constraints
{
    number 2;
